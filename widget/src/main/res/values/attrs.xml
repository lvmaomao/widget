<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="superrecyclerview">
        <attr name="layout_empty" format="reference" />
        <attr name="layout_moreProgress" format="reference" />
        <attr name="layout_progress" format="reference" />
        <attr name="recyclerClipToPadding" format="boolean" />
        <attr name="recyclerPadding" format="dimension" />
        <attr name="recyclerPaddingTop" format="dimension" />
        <attr name="recyclerPaddingBottom" format="dimension" />
        <attr name="recyclerPaddingLeft" format="dimension" />
        <attr name="recyclerPaddingRight" format="dimension" />
        <attr name="scrollbarStyle">
            <flag name="insideOverlay" value="0x0" />
            <flag name="insideInset" value="0x01000000" />
            <flag name="outsideOverlay" value="0x02000000" />
            <flag name="outsideInset" value="0x03000000" />
        </attr>

        <attr name="mainLayoutId" format="reference" />
    </declare-styleable>

    <declare-styleable name="SwipeLayout">
        <attr name="drag_edge" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="top" value="2" />
            <enum name="bottom" value="3" />
        </attr>
        <attr name="horizontalSwipeOffset" format="dimension" />
        <attr name="verticalSwipeOffset" format="dimension" />
        <attr name="show_mode" format="enum">
            <enum name="lay_down" value="0" />
            <enum name="pull_out" value="1" />
        </attr>
    </declare-styleable>

    <attr name="borderRadius" format="dimension" />
    <attr name="type">
        <enum name="circle" value="0" />
        <enum name="round" value="1" />
    </attr>

    <declare-styleable name="RoundImageView">
        <attr name="borderRadius" />
        <attr name="type" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AvatarLayout">
        <attr name="cover_dimen" format="dimension" />
        <attr name="unit_dimen" format="dimension" />
        <attr name="amount_count" format="integer" />
        <attr name="shape" format="enum">
            <enum name="circle" value="0" />
            <enum name="square" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RadarView">

        <attr name="firstRadarText" format="string" />
        <attr name="secondRadarText" format="string" />
        <attr name="thirdRadarText" format="string" />
        <attr name="fourRadarText" format="string" />
        <attr name="fiveRadarText" format="string" />

        <attr name="firstRadarDrawable" format="reference" />
        <attr name="secondRadarDrawable" format="reference" />
        <attr name="thirdRadarDrawable" format="reference" />
        <attr name="fourRadarDrawable" format="reference" />
        <attr name="fiveRadarDrawable" format="reference" />

    </declare-styleable>


    <attr name="text" format="string" />
    <attr name="textColor" format="color" />
    <attr name="textSize" format="dimension" />
    <attr name="barColor" format="color" />
    <attr name="rimColor" format="color" />
    <attr name="rimWidth" format="dimension" />
    <attr name="spinSpeed" format="dimension" />
    <attr name="delayMillis" format="integer" />
    <attr name="circleColor" format="color" />
    <attr name="pw_radius" format="dimension" />
    <attr name="progressBarWidth" format="dimension" />
    <attr name="progressBarLength" format="dimension" />
    <attr name="contourColor" format="color" />
    <attr name="contourSize" format="dimension" />

    <declare-styleable name="ProgressWheel">
        <attr name="text" />
        <attr name="textColor" />
        <attr name="textSize" />
        <attr name="barColor" />
        <attr name="rimColor" />
        <attr name="rimWidth" />
        <attr name="spinSpeed" />
        <attr name="delayMillis" />
        <attr name="circleColor" />
        <attr name="pw_radius" />
        <attr name="progressBarWidth" />
        <attr name="progressBarLength" />
        <attr name="contourColor" />
        <attr name="contourSize" />
    </declare-styleable>

    <declare-styleable name="ProgressWheel2">
        <attr name="text" />
        <attr name="textColor" />
        <attr name="textSize" />
        <attr name="secondText" format="string" />
        <attr name="secondTextColor" format="color" />
        <attr name="secondTextSize" format="dimension" />
        <attr name="barColor" />
        <attr name="rimColor" />
        <attr name="rimWidth" />
        <attr name="spinSpeed" />
        <attr name="delayMillis" />
        <attr name="circleColor" />
        <attr name="pw_radius" />
        <attr name="progressBarWidth" />
        <attr name="progressBarLength" />
        <attr name="contourColor" />
        <attr name="contourSize" />
    </declare-styleable>

    <declare-styleable name="DownloadView">
        <attr name="barColor" />
        <attr name="rimColor" />
        <attr name="rimWidth" />
        <attr name="spinSpeed" />
        <attr name="delayMillis" />
        <attr name="circleColor" />
        <attr name="pw_radius" />
        <attr name="progressBarWidth" />
        <attr name="progressBarLength" />
        <attr name="contourColor" />
        <attr name="contourSize" />
        <attr name="normalDrawable" format="reference" />
        <attr name="checkDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="PwdDisplayView">

        <attr name="flag" format="enum">
            <enum name="text" value="0" />
            <enum name="asterisk" value="1" />
            <enum name="circle" value="2" />
        </attr>

    </declare-styleable>


    <attr name="IconPageIndicatorStyle" format="reference" />

    <declare-styleable name="IndicatorView">

        <attr name="image" format="reference" />
        <attr name="indicatorTextSize" format="dimension" />
        <attr name="android:text" />
        <attr name="android:color" />

    </declare-styleable>

    <declare-styleable name="LinePageIndicator">
        <!-- Whether or not the indicators should be centered. -->
        <attr name="centered" format="boolean" />
        <!-- Color of the unselected lines that represent the pages. -->
        <attr name="unselectedColor" format="color" />
        <!-- Color of the selected line that represents the current page. -->
        <attr name="selectedColor" format="color" />
        <!-- Width of each indicator line. -->
        <attr name="lineWidth" format="dimension" />
        <!-- Width of each indicator line's stroke. -->
        <attr name="strokeWidth" format="dimension" />
        <!-- Width of the gap between each indicator line. -->
        <attr name="gapWidth" format="dimension" />
        <!-- View background -->
        <attr name="android:background" />
    </declare-styleable>

    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsDividerWidth" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsPaddingMiddle" format="boolean" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsTabNormalTextSize" format="dimension" />
        <attr name="pstsTabSelectTextSize" format="dimension" />
        <attr name="pstsTabTextColor" format="reference" />
        <attr name="pstsTabTextStyle" format="reference">
            <flag name="normal" value="0x0" />
            <flag name="bold" value="0x1" />
            <flag name="italic" value="0x2" />
        </attr>
        <attr name="pstsTabTextAllCaps" format="boolean" />
        <attr name="pstsTabTextAlpha" format="integer" />
        <attr name="pstsTabTextFontFamily" format="string" />
    </declare-styleable>

    <declare-styleable name="MarqueeLayout">
        <attr name="switchTime" format="integer" />
        <attr name="scrollTime" format="integer" />
        <attr name="orientation" format="enum">
            <enum name="up" value="1" />
            <enum name="down" value="2" />
            <enum name="left" value="3" />
            <enum name="right" value="4" />
        </attr>
        <attr name="enableAlphaAnim" format="boolean" />
        <attr name="enableScaleAnim" format="boolean" />
    </declare-styleable>

    <declare-styleable name="NiceSpinner">
        <attr name="arrowTint" format="color" />
        <attr name="hideArrow" format="boolean" />
        <attr name="backgroundSelector" format="integer" />
        <attr name="textTint" format="color" />
    </declare-styleable>

    <!--图片上传Loading-->
    <declare-styleable name="ProgressView">

        <attr name="progress_radius" format="dimension" />
        <attr name="progress_cricle_width" format="dimension" />
        <attr name="progress_fore_color" format="color" />
        <attr name="progress_back_color" format="color" />

    </declare-styleable>

    <declare-styleable name="AngleProgressBar">
        <attr name="angleHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="FixedWidthLinearLayout">
        <attr name="fixed_width_layout_width_weight" format="float" />
        <attr name="fixed_width_layout_remain" format="dimension" />
    </declare-styleable>
</resources>
